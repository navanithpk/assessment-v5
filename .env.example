# Copy this to .env and fill in your values

# Django Settings
SECRET_KEY=your-secret-key-here-generate-a-new-one
DEBUG=False
ALLOWED_HOSTS=localhost,127.0.0.1,.run.app

# Database (Cloud SQL)
DATABASE_URL=postgres://user:password@/dbname?host=/cloudsql/project:region:instance

# Google Cloud
GCP_PROJECT_ID=your-project-id
GOOGLE_CLOUD_SQL_INSTANCE=project:region:instance-name

# Optional: Cloud Storage for media files
GCS_BUCKET_NAME=your-bucket-name
